{
  "name": "npm-increaser-downloads",
  "version": "0.1.0",
  "description": "Raises the popularity score on your package through nefarious means.",
  "author": "MinhOmega",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "dependencies": {
    "date-fns": "2.28.0",
    "gaxios": "5.0.0",
    "ora": "5.4.1",
    "prompts": "2.4.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.2",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.7",
    "@types/prompts": "2.0.14",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "jest": "28.0.3",
    "jest-mock-console": "1.2.3",
    "nock": "13.2.4",
    "nodemon": "^3.1.4",
    "prettier": "2.6.2",
    "release-it": "^17.6.0",
    "ts-jest": "28.0.0",
    "ts-node": "^10.9.2",
    "typescript": "4.6.4"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node .",
    "test": "jest --coverage --detectOpenHandles",
    "format": "prettier . --write & eslint . --fix",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev:cli": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
    "release": "release-it"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/(coverage|lib).*/"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "commitMode": "direct",
      "push": true
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true,
      "web": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "bin": {
    "npm-increaser-downloads": "lib/src/cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**"
  ],
  "keywords": [
    "npm",
    "npmjs",
    "npms",
    "popularity",
    "increaser",
    "spammer",
    "download-simulator",
    "package-stats",
    "cli-tool",
    "typescript",
    "npm-increaser-downloads"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MinhOmega/npm-increaser-downloads.git"
  },
  "bugs": {
    "url": "https://github.com/MinhOmega/npm-increaser-downloads/issues"
  },
  "homepage": "https://github.com/MinhOmega/npm-increaser-downloads#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
